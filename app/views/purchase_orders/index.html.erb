<h1>Listing purchase_orders</h1>

<table>
  <tr>
    <th>Po number</th>
    <th>Company</th>
    <th>Date placed</th>
    <th>Date ready</th>
    <th>Date shipped</th>
    <th>Date received</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @purchase_orders.each do |purchase_order| %>
  <tr>
    <td><%= purchase_order.po_number %></td>
    <td><%= purchase_order.company_id %></td>
    <td><%= purchase_order.date_placed %></td>
    <td><%= purchase_order.date_ready %></td>
    <td><%= purchase_order.date_shipped %></td>
    <td><%= purchase_order.date_received %></td>
    <td><%= link_to 'Show', purchase_order %></td>
    <td><%= link_to 'Edit', edit_purchase_order_path(purchase_order) %></td>
    <td><%= link_to 'Destroy', purchase_order, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Purchase order', new_purchase_order_path %>
