<h1>Listing parts</h1>

<table>
  <tr>
    <th>Part number</th>
    <th>Description</th>
    <th>Column</th>
    <th>Column header</th>
    <th>Column color</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parts.each do |part| %>
  <tr>
    <td><%= part.part_number %></td>
    <td><%= part.description %></td>
    <td><%= part.column %></td>
    <td><%= part.column_header %></td>
    <td><%= part.column_color %></td>
    <td><%= link_to 'Show', part %></td>
    <td><%= link_to 'Edit', edit_part_path(part) %></td>
    <td><%= link_to 'Destroy', part, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Part', new_part_path %>
